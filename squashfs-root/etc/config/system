config system 'system'
	option hostname 'K2P'
	option timezone 'CST-8'
	option zonename 'NTPChinaCoas'
	option log_buffer_size '64'
	option loglevel '6'
    option hw_ver 'A1'
    option fw_ver '22.5.17.33'
    option up_ver '0'
    option support_app '1'
    option system 'openwrt'
	option boot '1'
	option domain 'p.to'

config timeserver 'ntp'
	list server 'clock.fmt.he.net'
	list server 'clock.nyc.he.net'
	list server 'cn.ntp.org.cn'
	list server 'tw.pool.ntp.org'
	option enabled '1'
	option enable_server '0'

config webauth 'weblogin'
        option username 'admin'
        option password 'YWRtaW4='
		
config autoupgrade 'autoupgrade'
	option up_type '1'
	option up_time '02:00-03:00'
	
config	history-type 'history'
	option num '5'
	option h1 'more_parentctl'
	option h1_num '1'
	option h1_stack '0'
	option h2 'more_signal'
	option h2_num '1'
	option h2_stack '0'
	option h3 'device_manage'
	option h3_num '1'
	option h3_stack '0'
	option h4 'more_wlextend'
	option h4_num '1'
	option h4_stack '0'
	option h5 'dhcp'
	option h5_num '1'
	option h5_stack '0'

config upgrade "upgrade"
	option wait_time '120000'
